<ul class="header-menu-left">
    {{#each leftCategory}}
        <li leftCategory-id="{{id}}" leftCate-title="{{title}}">{{~title~}}</li>
    {{/each}}
</ul>
<ul class="header-menu-right">
    {{#each rightCategory}}
        <li rightCategory-id="{{id}}" rightCategory-subId="{{subId}}" rightCategory-title="{{title}}">{{~title~}}
            {{#if subMenus}}
                <ul class="service-sub-menu">
                    {{#each subMenus}}
                        <li rightCategory-subId="{{subId}}" class="service-sub-list">{{subTitle}}</li>
                    {{/each}}
                </ul>
            {{/if}}
        </li>
    {{/each}}
</ul>